REM Assemble MS-DOS 2.0 from source.

MASM CHKDSK;
MASM CHKMES;
MASM CHKPROC;
LINK CHKDSK+CHKMES+CHKPROC;
EXE2BIN CHKDSK CHKDSK.COM
DEL CHKDSK.EXE
MASM DEBUG;
MASM DEBASM;
MASM DEBCOM1;
MASM DEBCOM2;
MASM DEBCONST;
MASM DEBDATA;
MASM DEBMES;
MASM DEBUASM;
LINK DEBUG+DEBASM+DEBCOM1+DEBCOM2+DEBCONST+DEBDATA+DEBMES+DEBUASM;
EXE2BIN DEBUG DEBUG.COM
DEL DEBUG.EXE
MASM DISKCOPY;
MASM DISKMES;
LINK DISKCOPY+DISKMES;
EXE2BIN DISKCOPY DISKCOPY.COM
DEL DISKCOPY.EXE
MASM EXE2BIN;
MASM EXEMES;
LINK EXE2BIN+EXEMES;
MASM EDLIN;
MASM EDLPROC;
MASM EDLMES;
LINK EDLIN+EDLMES+EDLPROC;
EXE2BIN EDLIN EDLIN.COM
DEL EDLIN.EXE
MASM FC;
MASM FCMES;
LINK FC+FCMES;
MASM FIND;
MASM FINDMES;
LINK FIND+FINDMES;
MASM MORE;
MASM MOREMES;
LINK MORE+MOREMES;
EXE2BIN MORE MORE.COM
DEL MORE.EXE
MASM PRINT;
LINK PRINT;
EXE2BIN PRINT PRINT.COM
DEL PRINT.EXE
MASM RECOVER;
MASM RECMES;
LINK RECOVER+RECMES;
EXE2BIN RECOVER RECOVER.COM
DEL RECOVER.EXE
MASM SORT;
MASM SORTMES;
LINK SORT+SORTMES;
MASM SYS;
MASM SYSMES;
LINK SYS+SYSMES;
EXE2BIN SYS SYS.COM
DEL SYS.EXE

